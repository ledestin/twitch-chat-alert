{
    "env": {
        "browser": true,
        "es2021": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": ["prettier"],
    "extends": ["eslint:recommended", "prettier"],
    "rules": {
      "prettier/prettier": "error",
      "camelcase": "error",
      "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
    }
}
